{
	"singleQuote": true,
	"semi": true,
	"useTabs": true,
	"trailingComma": "es5",
	"endOfLine": "auto",
	"printWidth": 120,
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"bracketSameLine": true,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"tabWidth": 2
}
